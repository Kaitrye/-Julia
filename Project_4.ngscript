        "using LinearAlgebra\r\n\r\n# Метод Ньютона для пункта d)\r\nfunction newton_method(F, J, x0; eps=1e-10, maxiter=1000, m=1)\r\n    x = copy(x0)\r\n    total = 0\r\n    jx = J(x)\r\n\r\n    for iter = 1:maxiter\r\n        # Обновляем матрицы\r\n        fx = F(x)\r\n        \r\n        if iter % m == 0 && iter != 1\r\n            jx = J(x)\r\n        end\r\n\r\n        # Обновляем x\r\n        Δx, cnt = solveLU(jx, -fx)\r\n        total += cnt\r\n        x += Δx\r\n\r\n        if norm(Δx) < eps\r\n            println(\"Количество итераций: $iter\\nКоличество ариф. операций: $total\")\r\n            return x\r\n        end\r\n    end\r\n\r\n    println(\"Метод Ньютона не сошелся\")\r\nend\r\n\r\nprintln(\"=== Метод Ньютона (пункт d) ===\\n\")\r\n\r\nfor i in 1:10\r\n    println(\"m = $i\")\r\n    @time x_a = newton_method(F, J, x0, m=i)\r\n    if x_a != nothing\r\n        println(x_a)\r\n        println(\"Проверка F(x) == 0 \", all(x -> isapprox(x, 0.0, atol=1e-10), F(x_a)))\r\n    end\r\n    println()\r\nend"
      ],
