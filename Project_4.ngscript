        "# Метод Ньютона для пункта c)\r\nfunction newton_method(F, J, x0; eps=1e-10, maxiter=100, k=Inf)\r\n    x = copy(x0)\r\n    total = 0\r\n    \r\n    if k <= 0\r\n        jx = J(x)\r\n    end\r\n\r\n    for iter = 1:maxiter\r\n        # Обновляем матрицы\r\n        fx = F(x)\r\n        \r\n        if iter <= k\r\n            jx = J(x)\r\n        end\r\n\r\n        # Обновляем x\r\n        Δx, cnt = solveLU(jx, -fx)\r\n        total += cnt\r\n        x += Δx\r\n\r\n        if norm(Δx) < eps\r\n            println(\"Количество итераций: $iter\\nКоличество ариф. операций: $total\")\r\n            return x\r\n        end\r\n    end\r\n\r\n    println(\"Метод Ньютона не сошелся\")\r\nend\r\n\r\nprintln(\"=== Метод Ньютона (пункт c) ===\\n\")\r\n\r\nfor i in 1:8\r\n    println(\"k = $i\")\r\n    @time x_a = newton_method(F, J, x0, k=i)\r\n    \r\n    if x_a != nothing\r\n        println(x_a)\r\n    end\r\n\r\n    println()\r\nend"
